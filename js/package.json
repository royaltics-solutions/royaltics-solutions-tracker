{
    "name": "@royaltics/tracker",
    "version": "0.0.20",
    "type": "module",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "vite build && tsc --emitDeclarationOnly",
        "test": "vitest",
        "test:coverage": "vitest --coverage",
        "test:hook": "vitest test/webhook",
        "prepublishOnly": "npm run build"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "keywords": [
        "tracker",
        "error-tracking",
        "monitoring",
        "logging",
        "typescript",
        "nodejs"
    ],
    "author": "Royaltics",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/royaltics-solutions/royaltics-solutions-tracker"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "typescript": "^5.0.0",
        "vite": "^5.0.0",
        "vite-plugin-dts": "^3.0.0",
        "vitest": "^1.0.0",
        "@vitest/coverage-v8": "^1.0.0"
    }
}