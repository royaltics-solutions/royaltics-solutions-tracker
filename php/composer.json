{
    "name": "royaltics/tracker-php",
    "description": "PHP client for Royaltics Error Tracker",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "tracker",
        "monitoring",
        "logging",
        "php"
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-zlib": "*",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Royaltics\\ErrorTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Royaltics\\ErrorTracker\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}